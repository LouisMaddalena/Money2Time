{
  "manifest_version": 3,
  "name": "Money2Time",
  "version": "1.0",
  "description": "Convert the prices on Amazon to time needed to work",
  "options_page": "options.html",
  "content_security_policy": {
},
  "background.service_worker": {
    "scripts": ["salary.js"],
    "persistent": false
  },
  "permissions": [
    "storage"
],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [ "options.html" ],
      "matches": [ "http://*/*", "https://*/*" ]
    }
  ]


}
